<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Telaingressos"
    android:background="@drawable/pranchet7">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dp"
        android:orientation="vertical">

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:background="@null" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="778dp"


            android:orientation="horizontal">

            <Button
                android:id="@+id/button1"
                android:layout_width="51dp"
                android:layout_height="32dp"
                android:background="@null"
                android:text="@string/menu"
                android:textColor="@color/corbranca"
                android:textSize="10dp" />

            <Button
                android:id="@+id/button3"
                android:layout_width="17dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:background="@null"
                android:text="@string/estado"
                android:textColor="@color/corbranca"
                android:textSize="10dp" />

            <Button
                android:id="@+id/button4"
                android:layout_width="17dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:background="@drawable/botaocolorido"
                android:text="@string/tok"
                android:textColor="@color/corbranca"
                android:textSize="10dp" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="7dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/icone29" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="7dp"
                android:layout_height="30dp"

                android:layout_weight="1"
                android:background="@null"
                android:scaleType="centerInside"

                app:srcCompat="@drawable/icone30" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="7dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/icone31" />
        </LinearLayout>



    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:paddingTop="100dp"
        android:paddingBottom="20dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true"
            android:text="@string/prompt_ingresso"
            android:textSize="20sp"
            android:textColor="@color/corbranca" />

    </LinearLayout>

    <ImageView
        android:id="@+id/icone13"
        android:layout_width="328dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="5dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:src="@drawable/icone13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icone14"
        android:layout_width="328dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="50dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="5dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:src="@drawable/icone14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icone18"
        android:layout_width="328dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="5dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:src="@drawable/icone18"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icone19"
        android:layout_width="328dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginTop="430dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="5dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:src="@drawable/icone19"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageButton
        android:id="@+id/tui"
        android:layout_width="66dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="97dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/icone6" />

    <ImageButton
        android:id="@+id/face"
        android:layout_width="66dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="53dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/icone23" />

    <ImageButton
        android:id="@+id/insta"
        android:layout_width="66dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="12dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/icone20" />

</RelativeLayout>